{
  "name": "TTGBot2",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npm run build && node lib/httpServer.js",
    "build": "npm run browserify & npm run less",
    "watch": "npm run browserify:watch & npm run less:watch",
    "prebuild": "mkdir -p build && cp lib/index.html build",
    "prewatch": "mkdir -p build && cp lib/index.html build",
    "browserify": "browserify -t reactify lib/index.js -o build/bundle.js --full-paths",
    "browserify:watch": "watchify -t reactify lib/index.js -o build/bundle.js --full-paths -v",
    "less": "lessc lib/less/main.less > build/main.css",
    "less:watch": "watch \"npm run less\" lib/less",
    "disc": "discify build/bundle.js > build/disc.html"
  },
  "devDependencies": {
    "watch": "^0.13.0",
    "browserify": "~8.1.3",
    "reactify": "~1.0.0",
    "watchify": "~2.3.0",
    "disc": "~1.3.1"
  },
  "dependencies": {
    "events": "^1.0.2",
    "express": "3.x",
    "flux": "^2.0.1",
    "object-assign": "^2.0.0",
    "react": "~0.12.2",
    "socket.io": "0.9.x",
    "bootstrap": "~3.1.0"
  }
}
